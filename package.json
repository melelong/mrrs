{
  "name": "mrrs",
  "private": true,
  "version": "0.0.3",
  "license": "MIT",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:service": "cd packages && cd mrrs_service && pnpm run start:dev",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "commit": "cz",
    "prepare": "husky install",
    "release": "standard-version",
    "lint": "eslint ./ --ext .ts,.js,.json --max-warnings=0",
    "lint:fix": "eslint ./ --ext .ts,.js,.json --max-warnings=0 --fix",
    "format": "prettier --config .prettierrc . --write",
    "format:dev": "onchange -d 1000 '**/*' -- prettier --config .prettierrc --write {{changed}}"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.5.2",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "prettier": "^3.0.0",
    "standard-version": "^9.5.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}