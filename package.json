{
  "name": "mrrs",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:service": "cd packages && cd mrrs_service && pnpm run start:dev",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "commit": "cz",
    "prepare": "husky install"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "dependencies": {
    "vue": "^3.3.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@vitejs/plugin-vue": "^4.5.2",
    "commitizen": "^4.3.0",
    "cz-customizable": "^7.0.0",
    "husky": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8",
    "vue-tsc": "^1.8.25"
  }
}